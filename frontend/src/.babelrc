{
  "presets": [
    "es2015",
    "react",
    "stage-2"
  ],
  "env": {
    "test": {
      "plugins": [
        "istanbul"
      ]
    }
  },
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd"
      }
    ],
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ]
  ]
}
